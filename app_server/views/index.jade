extends layout

block content
  h1= title
  p Welcome to #{title}
  #header-container
    header.wrapper.clearfix
      h1#title AdWords Dashboard Template
      nav
        ul
          li
            a(href='index.jade') Home
          li
            a(href='Notes.html') List Of Notes
          li
            a(href='#') Contact
  #main-container
    #main.wrapper.clearfix
      each d in data
        .col-xs-12.list-group

        table.table.table-bordered
          tr
            th Ad Group
            th Keyword
            th Impressions
            th Clicks
            th Match Type
            th Cost
            th Conversions
          tr
            td= d["Ad group"]
            td= d.Keyword
            td= d.Impressions
            td= d.Clicks
            td= d["Match type"]
            td= d.Cost
            td= d.Conversions






    // #main
  // #main-container



